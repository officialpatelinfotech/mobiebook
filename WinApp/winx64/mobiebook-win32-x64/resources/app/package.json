{
  "name": "mobiebook",
  "version": "0.0.0",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "node --openssl-legacy-provider node_modules/@angular/cli/bin/ng serve",
    "build": "node --openssl-legacy-provider node_modules/@angular/cli/bin/ng build",
    "test": "node --openssl-legacy-provider node_modules/@angular/cli/bin/ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "electron": "electron .",
    "electron-build": "node --openssl-legacy-provider node_modules/@angular/cli/bin/ng build --prod && electron .",
    "start:electron": "node --openssl-legacy-provider node_modules/@angular/cli/bin/ng build --base-href ./ && electron .",
    "electron-prod-build": "node --openssl-legacy-provider node_modules/@angular/cli/bin/ng build --prod --base-href ./ && electron-packager --out winx64 --overwrite --platform win32 --appname mobiebook . --icon=src\\assets\\img\\icon.ico"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "client-compress": "^2.2.2",
    "directory-tree": "^2.2.7",
    "exceljs": "^4.2.1",
    "file-saver": "^2.0.5",
    "image-size": "^1.1.1",
    "jimp": "^1.6.0",
    "ngx-electron": "^2.2.0",
    "pm2": "^6.0.14",
    "pngjs": "^7.0.0",
    "bwip-js": "^4.5.1",
    "qrcode": "^1.5.4",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@types/file-saver": "^2.0.2",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "electron": "^11.3.0",
    "electron-packager": "^15.2.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.1.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.2"
  }
